{
    "manifest_version": 3,
    "name": "Smart Job Autofill",
    "version": "1.0",
    "description": "Automatically detects and fills job application forms.",
    "permissions": ["storage", "scripting", "activeTab"],
    "options_ui": {
      "page": "Userdetail_ui/options.html",
      "open_in_tab": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["core_logic.js"]
      }
    ]
  }